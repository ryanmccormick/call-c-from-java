cc "-I/System/Library/Frameworks/JavaVM.framework/Versions/Current/Headers" 
"-I/System/Library/Frameworks/JavaVM.framework/Versions/A/Headers" -c HelloWorld.c



cc -c -I/System/Library/Frameworks/JavaVM.framework/Headers HelloWorld.c
cc -dynamiclib -o libhello.jnilib HelloWorld.o -framework JavaVM

cc -g -I/System/Library/Frameworks/JavaVM.framework/Headers -c -o HelloWorld.o HelloWorld.c


cc -bundle -o libhello.jnilib HelloWorld.o